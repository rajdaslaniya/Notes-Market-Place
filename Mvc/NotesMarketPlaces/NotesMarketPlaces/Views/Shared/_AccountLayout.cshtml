<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <!-- favicon -->
    <link rel="shortcut icon" href="~/Image/images/Favicon/favicon.ico" type="image/x-icon">
    <!--Link With font -->
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans
                 :wght@300;400;600;700&display=swap" rel="stylesheet">
    <!-- Fontawesome -->
    <link rel="stylesheet" href="~/Content/Front/css/fontawesome/css/font-awesome.min.css">
    <!--Bootstrap Css-->
    <link href="~/Content/Front/css/bootstrap/bootstrap.min.css" rel="stylesheet">
    <!--Style Css-->
    <link href="~/Content/Front/css/style.css" rel="stylesheet">
    <!--General Css-->
    <link href="~/Content/Front/css/geneal.css" rel="stylesheet">
    <!--Responsive Css-->
    <link href="~/Content/Front/css/responsive.css" rel="stylesheet">

</head>
<body>
    @RenderBody()
    <script src="~/Scripts/Front/js/jquery.js"></script>
    <script src="~/Scripts/Front/js/bootstrap/popper.min.js"></script>
    <script src="~/Scripts/Front/js/bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/Front/js/script.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>